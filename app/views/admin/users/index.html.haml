%table.table.table-striped.table-bordered
  %thead
    %tr
      %th ID
      %th Email
      %th % Completed 
      %th Actions

  %tbody
    - @users.each do |user|
      %tr
        %td= user.id
        %td= user.email
        %td= user.profile.progress_status
        %td
          = link_to 'Show', admin_user_path(user)
          = link_to 'Edit', edit_admin_user_path(user)
          = link_to 'Destroy', admin_user_path(user), :method => :delete, :data => { :confirm => 'Are you sure?' } unless user == current_user

%br

= link_to 'New User', new_admin_user_path
